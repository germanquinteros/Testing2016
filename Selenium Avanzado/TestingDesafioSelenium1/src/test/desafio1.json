{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://demo.opencart.com/"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "search"
      },
      "text": "iPhone"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//span[@class='input-group-btn']/button"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "grid-view"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='image']/a/img"
      }
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}